{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",<% if (enableTests || enableServerSupport) { %>
  "scripts": {<% if (enableServerSupport) { %>
    "start": "src/<%= multiple ? 'server/' : '' %>server.js"<% } %><% if (enableTests) { %>,
    "test": "./node_modules/grunt-cli/bin/grunt test"<% } %>
  },<% } %><% if (enableServerSupport) { %>
  "dependencies": {
    "express": "~3.4.0"
  },<% } %>
  "main": "src/<%= multiple ? 'server/' : '' %><%= enableServerSupport ? 'index' : 'server'%>.js",
  "devDependencies": {
    "grunt": "~0.4.0",
    "grunt-contrib-jshint": "~0.6.0",<% if (enableBrowserSupport) { %>
    "grunt-browserify": "~1.2.0",
    "debowerify": "~0.3.0",<% } if (enableBrowserSupport && enableServerSupport) { %>
    "less-middleware": "*",
    "browserify-middleware": "~1.19.0",<% } if (enableTests) { %>
    "grunt-mocha-cli": "~1.3.0",
    "mocha": "~1.14.0",
    "chai": "~1.8.0",
    "sinon": "~1.7.0",<% } %>
    "bower": "~0.9.0",
    "load-grunt-tasks": "~0.2.0",
    "jshint-stylish": "~0.1.3"
  }
}
